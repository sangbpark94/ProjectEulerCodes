import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;
import java.io.FileInputStream;

public class Main {
	
	public static void main(String[] args){
		
		Scanner sc = new Scanner(new File("input.txt"));
		String s = "";
		while(sc.hasNext()){
			s += sc.next();
		}
		System.out.println(s);
	}
	
}
