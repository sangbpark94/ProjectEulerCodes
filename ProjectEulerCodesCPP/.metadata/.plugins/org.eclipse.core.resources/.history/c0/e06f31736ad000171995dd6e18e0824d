#include <iostream>

using namespace std;

int calcPrime(int num);

int main(){
	int num = 20;
	int largestPrimes[21] = {0};
	int temp[21];
	for(int i = 2; i <= num; i++){
		temp = calcPrime(i);
		for(int j = 2; j <= 20; j++)
			if(temp[j] > largestPrimes[j])
				largestPrimes[j] = temp[j];
	}
	int sum = 0;
	for(int k = 2; k <= 20; k++)
		sum += k * largestPrimes[k];
	cout << sum;
}

int calcPrime(int num){
	int array[21] = {0};
	for(int i = 2; i <= num; i++)
		if(num % i == 0){
			array[i]++;
			num /= i;
			i = 2;
		}
	return array;
}

